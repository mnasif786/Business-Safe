                   namespace="BusinessSafe.Domain.Entities"
                   assembly="BusinessSafe.Domain">
  <class name="EmailTemplate" table="`EmailTemplate`">
    <id name="Id" column="Id" type="long">
      <generator class="identity"/>
    </id>

    <property name="Name" />
    <property name="Subject" />
    <property name="Body" />
    
  </class>
</hibernate-mapping>