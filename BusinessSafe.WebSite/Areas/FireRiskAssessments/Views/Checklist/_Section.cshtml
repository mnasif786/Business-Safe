@model BusinessSafe.WebSite.Areas.FireRiskAssessments.ViewModels.SectionViewModel
<fieldset id="@Model.ControlId" class="tab-pane @if(Model.Active) {<text> active</text> }">
    <legend>@Model.Title</legend>
    <ol class="section">
        @foreach(var question in Model.Questions)
        {
            @Html.Partial("_Question", question)
        }
    </ol>
</fieldset>
