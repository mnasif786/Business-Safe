        <div class= "row-fluid">
            <label class="wide">
                Site Name
            </label>
            <label class="massive">
                <strong>@Html.DisplayFor(model => model.SiteName)</strong>
            </label>
            <label class="wide">
                Person Seen
            </label>
            <label class="wide">
                <strong>@Html.DisplayFor(model => model.PersonSeen)</strong>
            </label>
            <label class="wide">
               Date of Visit
            </label>
            <label class="wide">
                <strong>@Html.DisplayFor(model => model.VisitDateFormatted)</strong>
            </label>
        </div>
        <div class="row-fluid">
            <label class="wide">
                Areas Visited
            </label>
            <label class="massive">
                <strong>@Html.DisplayFor(model => model.AreasVisited)</strong>
