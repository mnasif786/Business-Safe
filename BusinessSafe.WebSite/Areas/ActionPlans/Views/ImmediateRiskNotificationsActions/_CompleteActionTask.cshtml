@model BusinessSafe.WebSite.Areas.ActionPlans.ViewModels.CompleteActionTaskViewModel
@using (Html.BeginForm("Complete", "ImmediateRiskNotificationsActions", FormMethod.Post, new {id = "ActionTask", @class = "form-inline"}))
{
    <div class="readonly">      
        
        @Html.Partial("_ReadOnlyActionTaskDetail", Model)       
    </div>
    
    @Html.Partial("_CompleteActionTaskDetails", Model)

     <div class="row">
        <label class="span2">
        </label>
        <button type="submit" class="btn btn-primary disabled" id="TaskSaveButton">Complete Task</button>
        <button class="btn btn-link" id="TaskCancelButton">Cancel</button>
    </div>


    <script type="text/javascript">
        new SetFormToReadOnly('.readonly').execute();
    </script>
   
}
